{
  "name": "@sogebot/ui-overlay",
  "version": "15.0.0",
  "author": "sogehige@gmail.com",
  "engines": {
    "npm": ">7.19.0"
  },
  "description": "Nuxt.js version of ui-overlay",
  "scripts": {
    "postinstall": "patch-package || true",
    "release": "standard-version && git push --follow-tags origin main",
    "release:major": "standard-version -r major && git push --follow-tags origin main",
    "prepublishOnly": "npm run generate",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --fix --quiet --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --quiet --fix",
    "*.{css,vue}": "stylelint"
  },
  "simple-git-hooks": {
    "pre-commit": "npx simple-git-hooks && npx lint-staged",
    "commit-msg": "node ./.bot/tools/pre-commit-message.js $1 $2 $3",
    "preserveUnused": true
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@mdi/js": "^6.3.95",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@shubhrank/winwheeljs": "^2.8.0",
    "@sogebot/ui-helpers": "^2.7.2",
    "@types/uuid": "^8.3.1",
    "@vue/apollo-composable": "^4.0.0-alpha.15",
    "animate.css": "^4.1.1",
    "core-js": "^3.18.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^7.20.0",
    "gsap": "^3.8.0",
    "lint-staged": "^11.2.3",
    "nuxt": "^2.15.8",
    "obs-websocket-js": "~4.0.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "safe-eval": "^0.4.1",
    "standard-version": "^9.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "tiny-typed-emitter": "^2.1.0",
    "typeorm": "^0.2.38",
    "url-regex": "^5.0.0",
    "uuid": "^8.3.2",
    "v-runtime-template": "^1.10.0",
    "vue-json-viewer": "^2.2.19",
    "vuetify": "^2.5.10",
    "vuewordcloud": "^18.7.12"
  }
}
