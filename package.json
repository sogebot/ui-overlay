{
  "name": "@sogebot/ui-overlay",
  "version": "25.1.2",
  "author": "sogehige@gmail.com",
  "engines": {
    "npm": ">7.19.0"
  },
  "description": "Nuxt.js version of ui-overlay",
  "scripts": {
    "link-backend": "(npm link ../backend || (exit 0))",
    "postinstall": "(patch-package || (exit 0))",
    "release": "standard-version && git push --follow-tags origin main",
    "release:major": "standard-version -r major && git push --follow-tags origin main",
    "prepublishOnly": "npm run build",
    "dev": "npm run link-backend && cross-env PORT=3002 nuxi dev",
    "build": "npm run link-backend && nuxi generate",
    "start": "npm run link-backend && nuxi preview",
    "lint:js": "eslint --fix --quiet --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --quiet --fix",
    "*.{css,vue}": "stylelint"
  },
  "simple-git-hooks": {
    "pre-commit": "npx simple-git-hooks && npx lint-staged",
    "commit-msg": "node ./.bot/tools/pre-commit-message.js $1 $2 $3",
    "preserveUnused": true
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@mdi/js": "^6.6.96",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@3.0.0-27444169.120ee4f",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@shubhrank/winwheeljs": "^2.8.0",
    "@sogebot/ui-helpers": "^3.0.10",
    "@types/uuid": "^8.3.4",
    "animate.css": "^4.1.1",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.10.3",
    "lint-staged": "^12.3.7",
    "npm-check-updates": "^12.5.9",
    "nuxt-edge": "latest",
    "nuxt-graphql-request": "^5.1.0",
    "obs-websocket-js": "~4.0.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "safe-eval": "^0.4.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tiny-typed-emitter": "^2.1.0",
    "typeorm": "^0.2.45",
    "url-regex": "^5.0.0",
    "uuid": "^8.3.2",
    "v-runtime-template": "^1.10.0",
    "vue-json-viewer": "^2.2.22",
    "vuetify": "^2.6.4",
    "vuewordcloud": "^18.7.12",
    "worker-timers": "^7.0.46"
  }
}
